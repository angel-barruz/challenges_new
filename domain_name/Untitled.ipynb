   "source": [
    "# Extract the domain name from a URL\n",
    "\n",
    "def domain_name(url):\n",
    "    url= url.replace(\"http://\", \"\")\n",
    "    url= url.replace(\"www.\", \"\")\n",
    "    url= url.replace(\"https://\", \"\")\n",
    "    \n",
    "    return url.split(\".\")[0]\n"
   ]
  },
